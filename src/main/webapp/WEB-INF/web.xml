<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Deployment descriptor</display-name>

  <servlet>
    <servlet-name>Elvis</servlet-name>
    <servlet-class>com.elvis.business.ElvisHelloWorld</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis2</servlet-name>
    <servlet-class>com.elvis.business.ElvisHelloWorld2</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis3</servlet-name>
    <servlet-class>com.elvis.business.ElvisHelloWorld3</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis4</servlet-name>
    <servlet-class>com.elvis.business.ElvisFirstForm</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis5</servlet-name>
    <servlet-class>com.elvis.business.ElvisSecondForm</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis6</servlet-name>
    <servlet-class>com.elvis.business.ElvisCheckBox</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Elvis7</servlet-name>
    <servlet-class>com.elvis.business.ElvisReadParams</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>Elvis</servlet-name>
    <url-pattern>/Elvis</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis2</servlet-name>
    <url-pattern>/Elvis2</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis3</servlet-name>
    <url-pattern>/Elvis3</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis4</servlet-name>
    <url-pattern>/Elvis4</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis5</servlet-name>
    <url-pattern>/Elvis5</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis6</servlet-name>
    <url-pattern>/Elvis6</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Elvis7</servlet-name>
    <url-pattern>/Elvis7</url-pattern>
  </servlet-mapping>

</web-app>